{
  "expo": {
    "name": "Meal App",
    "slug": "TheMealApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/food-app-icon-new.png",
    "splash": {
      "image": "./assets/splash_meal.png",
      "resizeMode": "contain",
      "backgroundColor": "#40E0D0"
    },
    "updates": {
      "fallbackToCacheTimeout": 9000
    },
    "assetBundlePatterns": ["**/*"],

    "android": {
      "package": "com.prateek.mealsAppp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/food-app-icon-new.png",
        "backgroundColor": "#40E0D0"
      },
      "permissions": ["WRITE_EXTERNAL_STORAGE"]
    }
  }
}
